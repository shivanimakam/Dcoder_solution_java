import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    int n = s.nextInt();
    for(int i=0;i<n;i++)
    {
      int x= s.nextInt();
      int y = s.nextInt();
      if(y%x==0)
         System.out.println("Yes");
      else
         System.out.println("No");
    }
    
   }
 }
