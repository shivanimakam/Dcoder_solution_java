import java.util.*;

 // Compiler version JDK 11.0.2

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    String a = s.next();
    String b = s.next();
    
    int i = a.indexOf(b);
    if(i<0)
      System.out.println("0");
    else
    System.out.println("1");
   }
 }
