import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    int n = s.nextInt();
    if(n==1)
       System.out.println("1st");
    else if(n==2)
       System.out.println("2nd");
    else if(n==3)
       System.out.println("3rd");
    else
       System.out.println(n+"th");
     
   }
 }
