import java.util.*;
class Dcoder
{
    public static void main(String args[])
    {
        Scanner s = new Scanner(System.in);
        int n=s.nextInt();
        for(int i=1;i<=n;i++)
        {
          String str = s.next();
          if(str.equals("Nemo"))
            System.out.println(i);
        }
    }

}
