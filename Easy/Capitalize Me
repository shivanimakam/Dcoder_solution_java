import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    String str =s.next();
    str+=s.nextLine();
    String st[] = str.split(" ");
    for(int i=0;i<st.length;i++)
    {
      System.out.print(st[i].substring(0,1).toUpperCase()+st[i].substring(1)+" ");
    }
   }
 }
    
