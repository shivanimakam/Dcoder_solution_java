import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    String str = s.next();
    char ch[] = str.toCharArray();
    if(ch[0]==ch[4])
    {
      if(ch[1]==ch[3])
      {
        System.out.println("Yes");
      }
      else 
      System.out.println("No");
    }
         
    else
       System.out.println("No");
   }
 }
    
