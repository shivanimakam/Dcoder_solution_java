import java.util.*;

class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    int n= s.nextInt();
    for(int i=0;i<n;i++)
    {
      int y = s.nextInt();
      if (((y%4==0)&&(y%100!=0))||(y%400==0))
      {
        System.out.println("Yes");
      }
      else
      System.out.println("No");
    }
    
   }
 }
