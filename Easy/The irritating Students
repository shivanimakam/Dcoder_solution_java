import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    int n =s.nextInt();
    for(int i=0;i<n;i++)
    {
      int k = s.nextInt();
      if(k%2==0)
      {
        k=k/2;
        System.out.println(k+" "+k); 
      }
      else
      {
        k=k/2;
        System.out.println((k)+" "+(k+1));
      }
    }
    
   }
 }
    
