import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    int n = s.nextInt();
    for(int i=0;i<n;i++)
    {
      int z =s.nextInt();
      int r ;
     
      
        r=z%2;
        
      
      if(r==1)
         System.out.println("Yes");
      else
         System.out.println("No");
    }
    
   }
 }
