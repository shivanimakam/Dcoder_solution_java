import java.util.*;
class Dcoder
{
    satic int fact(int n)
    {
        if(n==1)
          return 1;
        return(n*fact(n-1));
    }
    public static void main(String args[])
    {
    Scanner s = new Scanner(System.in);
    int n=s.nectInt();
    int result = fact(n);
    System.out.println(result);
    }
}
