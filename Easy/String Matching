import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
      Scanner s = new Scanner(System.in);
      int n= s.nextInt();
      for(int i=0;i<n;i++)
      {
        String st = s.next();
        String st1 = s.next();
        if(st.contains(st1))
           System.out.println("Yes");
           else
           System.out.println("No");
      }
   }
 }
