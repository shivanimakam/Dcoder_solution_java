import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    String str = s.next();
    int n =str.length();
    char ch[] = str.toCharArray();
    
    Set<Character> st = new LinkedHashSet<Character>();
    for(int i=0;i<n;i++)
    {
      st.add(ch[i]);
    }
    Iterator itr = st.iterator();
    while(itr.hasNext())
    System.out.print(itr.next());
   }
 }
    
