import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
     Scanner s = new Scanner(System.in);
    String  n = s.next();
    int m=n.length();
    String fin="";
    for(int i=0;i<m;i++)
    {
      if(n.charAt(i)=='0')
         fin=fin+"1";
       else
         fin=fin+"0";
    }
    System.out.println(fin);
   }
 }
