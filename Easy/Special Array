import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    int n= s.nextInt();
    int a[] = new int[n];
    int flag=0;
    for(int i=0;i<n;i++)
    {
      a[i]=s.nextInt();
      if(a[i]==0)
         {
           flag=1;
           break;
         }
    }
    if(flag==1)
      System.out.println("No");
    else
      System.out.println("Yes");
   }
 }
