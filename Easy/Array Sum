import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
     Scanner s= new Scanner(System.in);
     int n = s.nextInt();
     int sum=0,max=0;
     int a[] = new int[n];
     for(int i=0;i<n;i++)
     {
       a[i]=s.nextInt();
       sum=sum+a[i];
       if(a[i]>max)
         max=a[i];
     }
     int rem = sum%max;
    System.out.println(rem);
   }
 }
