import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
     Scanner s = new Scanner(System.in);
     int n= s.nextInt();
     for(int i=0;i<n;i++)
     {
       int m=s.nextInt();
       int st=0,end;
       end =m%10;
       m=m/10;
       while(m>0)
       {
         st=m%10;
         m=m/10;
       }
       int x = st+end;
       System.out.println(x);
     }
    
   }
 }
