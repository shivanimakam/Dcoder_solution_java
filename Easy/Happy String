import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
     Scanner s = new Scanner(System.in);
     int n= s.nextInt();
     String str = "abcdefghijklmnopqrstuvwxyz";
     str=str.substring(0,n);
     char ch[] = str.toCharArray();
     for(int i=str.length()-1;i>=0;i--)
    System.out.print(ch[i]);
   }
 }
