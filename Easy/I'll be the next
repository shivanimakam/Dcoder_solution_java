import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    String str = s.next();
    char ch[] = str.toCharArray();
    for(int i= 0;i<str.length();i++)
    {
      int k = (char)ch[i];
      k=k+1;
      ch[i] = (char)k;
    }
    System.out.print(ch);
   }
 }
    
