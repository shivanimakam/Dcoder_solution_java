import java.util.*;


 class Dcoder
 {
   public static void main(String args[])
   { 
     Scanner s = new Scanner(System.in);
     int n = s.nextInt();
     for(int i=1;i<=n;i++)
     {
       for(int j=1;j<=i;j++)
       {
         if(j==i)
         System.out.print(j);
         else
         System.out.print(j+" ");
       }
       System.out.println();
     }
   }
 }
