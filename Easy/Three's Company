import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    int n = s.nextInt();
    String str = s.next();
    String st ="";
    for(int i=0;i<str.length();i++)
    {
      st=st+str.substring(i,i+1)+str.substring(i,i+1)+ str.substring(i,i+1);
        }
    System.out.println(st);
   }
 }
