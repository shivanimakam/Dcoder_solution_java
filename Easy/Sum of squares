import java.util.*;

 // Compiler version JDK 11.0.2

 class Dcoder
 {
   public static void main(String args[])
   { 
     Scanner s = new Scanner(System.in);
     int a = s.nextInt();
     int b = s.nextInt();
    int sum=0;
     for(int i=a;i<=b;i++)
       sum=sum+i*i;
     System.out.println(sum);
   }
 }
