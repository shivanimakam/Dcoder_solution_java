import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    int n= s.nextInt();
    for(int i=0;i<n;i++)
    {
      int a = s.nextInt();
      int b = s.nextInt();
      int c = s.nextInt();
      int k=(a+b+c)/2;
      int area = k*(k-a)*(k-b)*(k-c);
      int h = a+b+c;
      
      if(h==Math.sqrt(area))
         System.out.println("True");
      else
        System.out.println("False");
    }
    
   }
 }
    
