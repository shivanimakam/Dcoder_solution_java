import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    int n =s.nextInt();
    for(int i=0;i<n;i++)
    {
      int m =s.nextInt();
      int k= (int)Math.sqrt(m);
      int y = Math.abs(m-(k*k));
      int z = Math.abs(m-((k+1)*(k+1)));
     if(y<z)
          System.out.println(k);
      else
        System.out.println(k+1);
    }
    
   }
 }
    
