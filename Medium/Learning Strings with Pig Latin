import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
   // int n =s.nextInt();
   String str = s.next();
   char ch = str.charAt(0);
   if((ch=='a')||(ch=='e')||(ch=='i')||(ch=='o')||(ch=='u'))
   System.out.println(str+"way");
   else
    System.out.println(str.substring(1)+ch+"ay");
   }
 }
    
