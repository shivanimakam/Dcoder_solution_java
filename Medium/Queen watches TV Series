import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    Scanner s = new Scanner(System.in);
    int n =s.nextInt();
    for(int i=0;i<n;i++)
    {
      String str = s.next();
      int x = Integer.parseInt(str.substring(1,3));
      int y = Integer.parseInt(str.substring(4));
      System.out.println("Season "+x+", Episode "+y);
      
    }
    
   }
 }
    
